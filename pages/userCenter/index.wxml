<view class="msg-top">
  <image src='{{avatarUrl}}'></image>
  <view>
    {{nickName}}
  </view>
</view>
<view class="content">
  <view class='padding-border'>
    <navigator class="item flex-default" url="/pages/afterSale/mycar/index">
      <view class="icon-box">
        <image class="icon" src="/images/icons/personal_icon_car@3x.png" mode="widthFix"></image>
      </view>
      <text class="item-title">我的爱车</text>
      <view class="item-right flex1 flex-default flex-justify-content-end">
        <view class="icon-box">
          <image class="return-icon" src="/images/icons/mine_icon_return.png" mode="widthFix"></image>
        </view>
      </view>
    </navigator>
    <navigator class="item flex-default" url="/pages/userCenter/appointment/index">
      <view class="icon-box">
        <image class="icon" src="/images/icons/personal_icon_bespeak@3x.png" mode="widthFix"></image>
      </view>
      <text class="item-title">我的预约</text>
      <view class="item-right flex1 flex-default flex-justify-content-end">
        <view class="icon-box">
          <image class="return-icon" src="/images/icons/mine_icon_return.png" mode="widthFix"></image>
        </view>
      </view>
    </navigator>
    <navigator class="item flex-default" url="/pages/opinion/index">
      <view class="icon-box">
        <image class="icon" src="/images/icons/personal_icon_view@3x.png" mode="widthFix"></image>
      </view>
      <text class="item-title">意见反馈</text>
      <view class="item-right flex1 flex-default flex-justify-content-end">
        <view class="icon-box">
          <image class="return-icon" src="/images/icons/mine_icon_return.png" mode="widthFix"></image>
        </view>
      </view>
    </navigator>
     <navigator class="item flex-default" url='/pages/userCenter/msg/index'>
      <view class="icon-box">
        <image class="icon" src="/images/icons/personal_icon_view@3x.png" mode="widthFix"></image>
      </view>
      <text class="item-title">个人信息</text>
      <view class="item-right flex1 flex-default flex-justify-content-end">
        <view class="icon-box">
          <image class="return-icon" src="/images/icons/mine_icon_return.png" mode="widthFix"></image>
        </view>
      </view>
    </navigator>
  </view>
</view>