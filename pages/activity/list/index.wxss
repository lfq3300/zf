.move-menu{
  border-bottom: 1px solid #DCDCDC;
  margin-bottom: 48rpx;
}

.move-menu .tag{
  padding-left: 6.25%;
  padding-right:  6.25%;
}
.move-menu .select text::after{
  bottom: -10rpx;
}

.act-list{
  overflow: hidden;
}
.act-list image{
  width: 100%;
  height: 312rpx;
}
.act-list  .title{
  margin-top: 28rpx;
  color: #3E3A39;
  font-size: 24rpx;
}
.act-list  .date{
  color:#DCDCDC;
  margin-top: 18rpx;
  font-size: 16rpx;
}
.act-list navigator{
  border-bottom: 1px solid #DCDCDC;
  padding-bottom: 10rpx;
  margin-bottom: 100rpx;
}